<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:ignore="MissingClass">

    <ImageView
        android:id="@+id/transfer_back"
        android:layout_width="48dp"
        android:layout_height="24dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="354dp"
        android:layout_marginBottom="669dp"
        android:layout_marginRight="354dp"
        android:rotation="180"
        android:src="@drawable/back" />

    <TextView
        android:id="@+id/main_transfer"
        android:layout_width="46dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="203dp"
        android:layout_marginBottom="663dp"
        android:text="송금"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        android:layout_marginRight="203dp"
        android:layout_marginLeft="60dp" />


    <EditText
        android:id="@+id/editText_from"
        android:layout_width="480dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="170dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="549dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="154dp"
        android:layout_marginEnd="283dp"
        android:layout_marginBottom="550dp"
        android:text="보내는 계좌"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/editText_to"
        android:layout_width="480dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="170dp"
        android:layout_marginTop="202dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="497dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="207dp"
        android:layout_marginEnd="302dp"
        android:layout_marginBottom="497dp"
        android:text="받는 계좌"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/editText_amount"
        android:layout_width="480dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="170dp"
        android:layout_marginTop="257dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="442dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="262dp"
        android:layout_marginEnd="342dp"
        android:layout_marginBottom="442dp"
        android:text="금액"
        android:textSize="20sp"/>

    <Button
        android:id="@+id/Transfer"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="333dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="337dp"
        android:background="@drawable/border_button"
        android:text="이체"
        android:textSize="20sp"
        android:textStyle="bold" />

</RelativeLayout>